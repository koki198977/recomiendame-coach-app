{
  "expo": {
    "name": "Recomiéndame Coach",
    "slug": "recomiendame-coach-app",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La app necesita acceso a tus fotos para subir imágenes de comidas y equipamiento.",
          "cameraPermission": "La app necesita acceso a tu cámara para tomar fotos de tus comidas."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#4CAF50"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "cl.recomiendameapp.coach",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "youtube",
          "vnd.youtube"
        ]
      }
    },
    "android": {
      "package": "cl.recomiendameapp.coach",
      "versionCode": 101,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4CAF50"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET"
      ],
      "softwareKeyboardLayoutMode": "pan",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https"
            },
            {
              "scheme": "http"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "statusBar": {
        "backgroundColor": "#ffffff",
        "barStyle": "dark-content"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b0932931-1a3a-4f83-9a05-6b11a2a6e8e5"
      }
    },
    "owner": "kokialvarez78"
  }
}
